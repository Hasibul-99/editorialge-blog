---
import { IoSearchOutline } from "react-icons/io5";
import LeftToggleMenu from "./LeftToggleMenu";
import MobileMenuToggle from "./MobileMenuToggle";
import Logo from "../../../assets/img/logo/logo.png";
import WLogo from "../../../assets/img/logo/w_logo.png";
---

<div id="sticky-header" class="menu-area menu-style-six">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <div class="menu-wrap">
          <nav class="menu-nav">
            <div class="logo d-none">
              <a href="/"><img src={Logo.src} alt="" /></a>
            </div>
            <div class="logo d-none white-logo">
              <a href="/"><img src={WLogo.src} alt="" /></a>
            </div>
            <div class="offcanvas-toggle">
              <LeftToggleMenu client:load />
            </div>
            <div class="navbar-wrap main-menu d-none d-lg-flex">
              <ul class="navigation">
                <li class="active menu-item-has-children">
                  <a href="#">Home</a>
                  <ul class="sub-menu">
                    <li><a href="/">Home 01 - Default</a></li>
                    <li><a href="/">Home 02 - Gaming</a></li>
                    <li><a href="/">Home 03 - Technology</a></li>
                    <li><a href="/">Home 04 - Travel</a></li>
                    <li><a href="/">Home 05 - Crypto</a></li>
                    <li class="active"><a href="/">Home 06 - Newspaper</a></li>
                  </ul>
                </li>
                <li><a href="">About Us</a></li>
                <li class="menu-item-has-children">
                  <a href="#">Features</a>
                  <ul class="sub-menu">
                    <li class="menu-item-has-children">
                      <a href="#">Single Post Layout</a>
                      <ul class="sub-menu">
                        <li><a href="">Single post 01</a></li>
                        <li><a href="">Single post 02</a></li>
                      </ul>
                    </li>
                    <li><a href="">Author Details</a></li>
                  </ul>
                </li>
                <li class="menu-item-has-children">
                  <a href="#">Categories</a>
                  <ul class="sub-menu">
                    <li><a href="">Blog Default</a></li>
                    <li><a href="">Blog Layout 02</a></li>
                    <li><a href="">Blog Layout 03</a></li>
                  </ul>
                </li>
                <li><a href="/contact">Contact</a></li>
              </ul>
            </div>
            <div class="header-search-wrap header-search-wrap-three">
              <form action="#">
                <input type="text" placeholder="Search here . . ." />
                <button type="submit"><IoSearchOutline /></button>
              </form>
            </div>
            <div class="mobile-nav-toggler">
              <!-- <FaBars /> -->
              <MobileMenuToggle client:load />
            </div>
          </nav>
        </div>
      </div>
    </div>
  </div>
</div>
