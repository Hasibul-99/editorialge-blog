---
import logo from "../../assets/img/logo/logo.png";
import wLogo from "../../assets/img/logo/w_logo.png";
import { LanguageSelector } from "astro-i18next/components";
const {
  icon = "",
  languageMapping = { en: "English", it: "Italiano" },
  ...rest
} = Astro.props;
---

<div class="header-logo-area-four">
  <div class="container">
    <div class="row align-items-center">
      <div class="col-lg-4">
        <div class="hl-left-side-four">
          <span class="date"
            ><i class="flaticon-calendar"></i>February 12, 2024</span
          >
        </div>
      </div>
      <div class="col-lg-4">
        <div class="logo text-center">
          <a href="index.html">
            <img src={logo.src} alt="" />
          </a>
        </div>
        <div class="logo d-none text-center">
          <a href="index.html">
            <img src={wLogo.src} alt="" />
          </a>
        </div>
      </div>
      <div class="col-lg-4">
        <div class="hl-right-side-four">
          <LanguageSelector
            showFlag={false}
            languageMapping={languageMapping}
            class="appearance-none cursor-pointer pl-3 pr-2 py-1.5 rounded-md dark:bg-stone-950 dark:text-white focus-visible:outline-none"
            aria-label="Choose a language"
          />
          <div class="sign-in">
            <a href="contact.html"><i class="flaticon-user"></i>Sign In</a>
          </div>
          <!-- <div class="subscribe-btn">
                        <a href="contact.html" class="btn btn-two">Subscribe</a>
                    </div> -->
        </div>
      </div>
    </div>
  </div>
</div>
